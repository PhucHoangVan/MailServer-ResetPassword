
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                Int                @id @map("id")
  name              String?            @map("name")
  email             String             @unique @map("email")
  password          String             @map("password")

  @@map("account")
}

